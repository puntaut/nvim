return {
    "dstein64/nvim-scrollview",
    event = "BufReadPre",
    config = function()
        require('scrollview').setup({
            excluded_filetypes = { 'NvimTree' }
        })
        -- vim.g.scrollview_excluded_filetypes = { 'NvimTree' }
        require('scrollview.contrib.gitsigns').setup()
    end
}
